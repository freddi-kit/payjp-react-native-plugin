{
  "name": "payjp-api-client",
  "version": "1.0.0",
  "description": "api generator for PAY.JP",
  "author": "yatatsu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "clean": "rimraf dist/",
    "watch": "tsc -p . --watch",
    "validate": "openapi-generator validate -i payjp.yaml",
    "generate": "openapi-generator generate -i payjp.yaml -g typescript-fetch"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^0.0.19-4.1.2",
    "typescript": "^3.5.3"
  }
}
