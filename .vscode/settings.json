{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.nodePath": "node_modules/eslint",
  "eslint.run": "onSave",
  "eslint.alwaysShowStatus": true,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.words": [
    "livemode",
    "payjp",
    "podspec"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}